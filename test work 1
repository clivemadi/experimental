f= open("lists.txt","w+")
for i in range(12):
     f.write("This is code %d\r\n" % (i+1))
     f.close()
     
f=open("lists.txt", "a+")
for i in range(2):
     f.write("Appended code %d\r\n" % (i+1))

f=open("lists.txt", "r")
if f.mode == 'r':
     contents =f.read()
     print(contents)
if __name__== "__main__"
main()


